class Animal
{
Animal()
{
// Este mensaje se muestra cuando se crea un Animal
System.out.println("Animal is created");
}
}

class Dog extends Animal
{
Dog()
{
super();
// Llama primero al constructor de Animal

// Este mensaje se muestra cuando se crea un Dog
System.out.println("Dog is created");
}
}

public class Main
{
public static void main(String[] args)
{
// Se crea un objeto de tipo Dog
Dog myDog = new Dog();

// Al crearlo, se ejecuta primero el mensaje de Animal
// y despu√©s el mensaje de Dog
}
}
